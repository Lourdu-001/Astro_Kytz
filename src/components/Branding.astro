<script>
    /* _______________ FEATURED BRANDING ______________
        --------------------------------------------------- */
    var branding_box = document.querySelectorAll(".branding-box");
    var change_ani_cover = document.querySelectorAll(".change-ani-cover");
    var educational_images = document.querySelectorAll(".gan");

    branding_box.forEach((pairedData, pairedIndex) => {
        
        pairedData.onmouseover=() => {
            change_ani_cover[pairedIndex].style.backgroundColor="#C9F001";

            if(pairedIndex == 0) {
                educational_images[0].classList.add("addeducation");
            }
            if(pairedIndex == 1) {
                educational_images[1].classList.add("addcyber");
            }
            if(pairedIndex == 2) {
                educational_images[2].classList.add("addtech");
            }
        }

        pairedData.onmouseleave=() => {
            change_ani_cover[pairedIndex].style.backgroundColor="#1A182C";

            if(pairedIndex == 0) {
                educational_images[0].classList.remove("addeducation");
            }
            if(pairedIndex == 1) {
                educational_images[1].classList.remove("addcyber");
            }
            if(pairedIndex == 2) {
                educational_images[2].classList.remove("addtech");
            }
        }
    })
</script>