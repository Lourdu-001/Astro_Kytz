
<style>
   /* -------------------- FEATURED BRANDING ----------------
______________________________________________________ */

    .new_class{
        font-family: Inter;
        font-size: 28px;
        font-weight: 500;
        line-height: 33.89px;
        letter-spacing: -0.03em;
        text-align: left;
    }

    .featured-branding{
        background-color: #06041A;
    }

    .educational-images{
        height: 300px;
        background-image: url("../../public/educational_brands/education_brand1.webp");
        background-size: cover;
        width: 100%;
        border-radius: 0 0 10px 10px;
        background-position: center;
    }
    .cyber-security-images{
        height: 300px;
        background-image: url("../../public/cyber_security_branding/cyber-security1.webp");
        background-size: cover;
        width: 100%;
        border-radius: 0 0 10px 10px;
        background-position: center;
    }
    .tech-company{
        height: 300px;
        background-image: url("../../public/tech_company/gaming_application1.webp");
        background-size: cover;
        width: 100%;
        border-radius: 0 0 10px 10px;
        background-position: center;
    }
    .change-ani-cover{
        background-color: #1A182C;
        border-radius: 10px 10px 0 0;
    }
    .branding-stroke-button{
        font-size: 12px;
        font-weight: 500;
        /* color: #1E1E1E;  */
        color: #1E1E1E;
        border-radius: 50px;
        border: 1px solid #1E1E1E;
    }
    .addeducation{
        animation: bgeducation 2s infinite;
    }

    @keyframes bgeducation {
        0%{
            background-image: url("../../public/educational_brands/education_brand1.webp");
        }
        20%{
            background-image: url("../../public/educational_brands/education_brand1.webp");
        }
        40%{
            background-image: url("../../public/educational_brands/education_brand2.webp");
        }
        60%{
            background-image: url("../../public/educational_brands/education_brand2.webp");
        }
        80%{
            background-image: url("../../public/educational_brands/education_brand3.webp");
        }
        100%{
            background-image: url("../../public/educational_brands/education_brand3.webp");
        }
    }

    .addcyber{
        animation: bgcyber 2s infinite;
    }

    @keyframes bgcyber {
        0%{
            background-image: url("../../public/cyber_security_branding/cyber-security1.webp");
        }
        20%{
            background-image: url("../../public/cyber_security_branding/cyber-security1.webp");
        }
        40%{
            background-image: url("../../public/cyber_security_branding/cyber-security2.webp");
        }
        60%{
            background-image: url("../../public/cyber_security_branding/cyber-security2.webp");
        }
        80%{
            background-image: url("../../public/cyber_security_branding/cyber-security3.webp");
        }
        100%{
            background-image: url("../../public/cyber_security_branding/cyber-security3.webp");
        }
    }

    .addtech{
        animation: bgtech 2s ease infinite;
    }

    @keyframes bgtech {
        0%{
            background-image: url("../../public/tech_company/gaming_application1.webp");
        }
        20%{
            background-image: url("../../public/tech_company/gaming_application1.webp");
        }
        40%{
            background-image: url("../../public/tech_company/gaming_application2.webp");
        }
        60%{
            background-image: url("../../public/tech_company/gaming_application2.webp");
        }
        80%{
            background-image: url("../../public/tech_company/gaming_application3.webp");
        }
        100%{
            background-image: url("../../public/tech_company/gaming_application3.webp");
        }
    }

    @media (max-width: 600px) {
        .h2{
            font-size: 26px;
        }
        .identity-brand{
            display: none;
        }
        .h4{
            font-size: 22.23px;
        }
    }

</style>
